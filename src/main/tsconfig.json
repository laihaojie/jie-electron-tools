{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../build/main",
    "allowJs": true,
    "noImplicitAny": false
  },
  "include": ["./**/*.ts", "./**/*.d.ts", "../types.d.ts"],
  "exclude": ["static"]
}
